<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.binish.guitartransposer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/guitar_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/guitar_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainPageV2"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_main_page_v2"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainPageUpdated"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>